<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence           http://xmlns.jcp.org/xml/ns/persistence/persistence_2_2.xsd">
  <persistence-unit name="AcademicPlusPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>modelo.Eventos</class>
    <class>modelo.ComentariosRevisionTaller</class>
    <class>modelo.BitacorasEventos</class>
    <class>modelo.EventoParticipantesTalleres</class>
    <class>modelo.Evidencias</class>
    <class>modelo.Notificaciones</class>
    <class>modelo.Convocatorias</class>
    <class>modelo.Talleres</class>
    <class>modelo.Usuarios</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/academicplus?serverTimezone=America/Mexico_City&amp;zeroDateTimeBehavior=CONVERT_TO_NULL"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.cj.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.password" value="password"/>
    </properties>
  </persistence-unit>
</persistence>
